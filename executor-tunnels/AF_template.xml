<?xml version='1.0' encoding='utf-8'?>
<Tunnels>
  <Input>/path/to/structure.cif</Input>
  <WorkingDirectory>/path/to/workdir</WorkingDirectory>
  <Params>
    <Cavity ProbeRadius="5" InteriorThreshold="1.1" IgnoreHETAtoms="1"  />
    <Tunnel 
      SurfaceCoverRadius="10" 
      OriginRadius="5" 
      BottleneckRadius="1.2" 
      BottleneckTolerance="3" 
      MaxTunnelSimilarity="0.7" 
      WeightFunction="VoronoiScale" />
  </Params>
  <Export>
    <Formats PyMol="1" CSV="1" PDBProfile="1" PDBStructure="1" JSON="1" />
    <Types Cavities="1" Tunnels="1" />
    <PyMol SurfaceType="Surface" />
  </Export>
  <Origins Auto="0">
  
  <Origin>
	  <Point X="0.0" Y="0.0" Z="0.0" />
  </Origin>
  </Origins>
</Tunnels>
